{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "height": 305,
  "width": "container",
  "data": {
    "url": "https://raw.githubusercontent.com/RDeconomist/observatory/main/O20_C1.csv"
  },
  "title": {
    "text": "Ukrainian real growth",
    "align": "left",
    "anchor": "start",
    "subtitle": ["Real annual GDP growth", "Source: IMF", ""]
  },
  "config": {
    "font": "Arial",
    "mark": {"line": {"interpolate": "linear"}},
    "view": {"stroke": "transparent", "width": 400, "height": 300},
    "range": {
      "category": [
        "#001f3f",
        "#4269d0",
        "#efb118",
        "#ff725c",
        "#6cc5b0",
        "#3ca951",
        "#ff8ab7",
        "#a463f2",
        "#97bbf5",
        "#9c6b4e"
      ],
      "diverging": ["#E6224B", "#E54753", "#C9C9C9", "#179FDB", "#122B39"],
      "heatmap": ["#C9C9C9", "#179FDB", "#0063AF", "#122B39"],
      "ordinal": ["#00A767", "#36B7B4", "#179FDB", "#0063AF", "#243B5A"]
    },
    "axisX": {
      "labelColor": "#122b39",
      "tickColor": "#122b39",
      "domainColor": "#122b39",
      "domainOpacity": 0.5,
      "gridOpacity": 0,
      "labelFont": "Arial",
      "labelAngle": 0,
      "labelAlign": "center",
      "labelFontSize": 11,
      "labelPadding": 5,
      "tickCount": 10,
      "tickSize": 0,
      "title": ""
    },
    "axisY": {
      "labelColor": "#122b39",
      "labelFont": "Arial",
      "tickColor": "#122b39",
      "domainColor": "#122b39",
      "gridColor": "#122b39",
      "gridDash": [1, 5],
      "gridOpacity": 0.5,
      "labelPadding": 5,
      "labelFontSize": 11,
      "domainOpacity": 0.5,
      "tickSize": 0,
      "title": null,
      "titleAlign": "left",
      "titleAngle": 0,
      "titleBaseline": "bottom",
      "titleColor": "#122b39",
      "titleOpacity": 0.9,
      "titleX": 0,
      "titleY": -7
    }
  },
  "mark": {"type": "line", "color": "#e54753", "size": 2.75},
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "sort": [],
      "title": null,
      "axis": {"grid": false, "labelAngle": 0}
    },
    "y": {
      "field": "value2",
      "type": "quantitative",
      "title": "CPI",
      "scale": {"domain": [-0.4, 0.15]},
      "axis": {
        "grid": true,
        "format": ".0%",
        "labelFontSize": 11,
        "title": "",
        "titleAlign": "left",
        "titleAngle": 0,
        "titleBaseline": "bottom",
        "titleFontSize": 12,
        "titleX": 5,
        "titleY": 15
      }
    },
    "tooltip": [
      {
        "field": "date",
        "type": "temporal",
        "timeUnit": "yearmonth",
        "title": "Date"
      },
      {
        "field": "value2",
        "type": "quantitative",
        "format": ".1%",
        "title": "Real GDP growth"
      }
    ]
  }
}